export LD_LIBRARY_PATH
SYBASE_ASE=ASE-12_5
export SYBASE_ASE
LIBPATH=/sw/sybase/OCS-12_5/lib:/sw/sybase/OCS-12_5/lib3p:/usr/lib:$LIBPATH
export LIBPATH
SYBASE_OCS=OCS-12_5
export SYBASE_OCS
export SYBEDITOR=vi
export MAXTEXTLEN=8192
export SYB_MINDECIMAL=-5000
export SYB_MAXDECIMAL=5000
export SYBASE_TERM=xterm_c.ibm
export SYBFORMLIST=$HOME/f
export SYBPLATFORM=rs6000
export LANG=En_US
echo
