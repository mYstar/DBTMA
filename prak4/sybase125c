stty erase ^?
export SYBASE=/sw/sybase
export DSQUERY=syb125c
export DBBUFSIZE=1500
export DSCONSOLE=syb125c
export DSLISTEN=syb125c
export LPDEST=lp 
export SQLBUFFERLEN=8192
